@model ParkingLotApp.WebUI.ViewModels.AddParkingSpaceViewModel

<h1>@Model.parkingLot.Address</h1>
<div>
    <p>@Model.parkingLot.Location</p>
    <p>@Model.parkingLot.Floor</p>
    <p>@Model.parkingLot.Spaces</p>

    
</div>

@Html.DropDownListFor(x => x.parkingSpace, Model.ParkingSpaces)

<select class="form-control" id="parkingSpace" name="parkingSpace">
    <option>Select Space</option>
    
</select>

<table class="table">
    <tr>
        <th>Start</th>
        <th>Finish</th>
        <th>Rate</th>
        <th>Location</th>
    </tr>

